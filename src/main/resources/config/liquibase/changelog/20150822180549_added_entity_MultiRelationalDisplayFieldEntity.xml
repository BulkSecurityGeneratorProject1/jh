<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql"/>
    <property name="autoIncrement" value="false" dbms="oracle"/>

    <property name="floatType" value="real" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle"/>

    <!--
        Added the entity MultiRelationalDisplayFieldEntity.
    -->
    <changeSet id="20150822180549" author="jhipster">
        <createTable tableName="MULTIRELATIONALDISPLAYFIELDENTITY">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}" >
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="onetoonedisplayfieldentity_id" type="bigint">
                <constraints unique="true"/>
            </column>
            <column name="onetomanydisplayfieldentity_id" type="bigint"/>
        </createTable>
        
        <addForeignKeyConstraint baseColumnNames="onetoonedisplayfieldentity_id"
                                 baseTableName="MULTIRELATIONALDISPLAYFIELDENTITY"
                                 constraintName="fk_multirelationaldisplayfieldentity_onetoonedisplayfieldentity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ONETOONEDISPLAYFIELDENTITY"/>

        <createTable tableName="MULTIRELATIONALDISPLAYFIELDENTITY_MANYTOMANYDISPLAYFIELDENTITY">
            <column name="manytomanydisplayfieldentitys_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="multirelationaldisplayfieldentitys_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addPrimaryKey columnNames="multirelationaldisplayfieldentitys_id, manytomanydisplayfieldentitys_id" tableName="MULTIRELATIONALDISPLAYFIELDENTITY_MANYTOMANYDISPLAYFIELDENTITY"/>

        <addForeignKeyConstraint baseColumnNames="multirelationaldisplayfieldentitys_id"
                                 baseTableName="MULTIRELATIONALDISPLAYFIELDENTITY_MANYTOMANYDISPLAYFIELDENTITY"
                                 constraintName="fk_multirelationaldisplayfieldentity_manytomanydisplayfieldentity_manytomanydisplayfieldentity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="MULTIRELATIONALDISPLAYFIELDENTITY"/>

        <addForeignKeyConstraint baseColumnNames="manytomanydisplayfieldentitys_id"
                                 baseTableName="MULTIRELATIONALDISPLAYFIELDENTITY_MANYTOMANYDISPLAYFIELDENTITY"
                                 constraintName="fk_manytomanydisplayfieldentity_manytomanydisplayfieldentity_multirelationaldisplayfieldentity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="MANYTOMANYDISPLAYFIELDENTITY"/>
        
        <addForeignKeyConstraint baseColumnNames="onetomanydisplayfieldentity_id"
                                 baseTableName="MULTIRELATIONALDISPLAYFIELDENTITY"
                                 constraintName="fk_multirelationaldisplayfieldentity_onetomanydisplayfieldentity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ONETOMANYDISPLAYFIELDENTITY"/>

    </changeSet>
</databaseChangeLog>
