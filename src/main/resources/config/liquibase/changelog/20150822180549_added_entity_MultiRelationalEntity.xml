<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql"/>
    <property name="autoIncrement" value="false" dbms="oracle"/>

    <property name="floatType" value="real" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle"/>

    <!--
        Added the entity MultiRelationalEntity.
    -->
    <changeSet id="20150822180549" author="jhipster">
        <createTable tableName="MULTIRELATIONALENTITY">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}" >
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="onetooneentity_id" type="bigint">
                <constraints unique="true"/>
            </column>
            <column name="onetomanyentity_id" type="bigint"/>
        </createTable>
        
        <addForeignKeyConstraint baseColumnNames="onetooneentity_id"
                                 baseTableName="MULTIRELATIONALENTITY"
                                 constraintName="fk_multirelationalentity_onetooneentity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ONETOONEENTITY"/>

        <createTable tableName="MULTIRELATIONALENTITY_MANYTOMANYENTITY">
            <column name="manytomanyentitys_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="multirelationalentitys_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>

        <addPrimaryKey columnNames="multirelationalentitys_id, manytomanyentitys_id" tableName="MULTIRELATIONALENTITY_MANYTOMANYENTITY"/>

        <addForeignKeyConstraint baseColumnNames="multirelationalentitys_id"
                                 baseTableName="MULTIRELATIONALENTITY_MANYTOMANYENTITY"
                                 constraintName="fk_multirelationalentity_manytomanyentity_manytomanyentity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="MULTIRELATIONALENTITY"/>

        <addForeignKeyConstraint baseColumnNames="manytomanyentitys_id"
                                 baseTableName="MULTIRELATIONALENTITY_MANYTOMANYENTITY"
                                 constraintName="fk_manytomanyentity_manytomanyentity_multirelationalentity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="MANYTOMANYENTITY"/>
        
        <addForeignKeyConstraint baseColumnNames="onetomanyentity_id"
                                 baseTableName="MULTIRELATIONALENTITY"
                                 constraintName="fk_multirelationalentity_onetomanyentity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ONETOMANYENTITY"/>

    </changeSet>
</databaseChangeLog>
